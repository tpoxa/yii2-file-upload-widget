{
    "name": "mtrofimenko/yii2-file-upload-widget",
    "version": "1.0.5",
    "description": "Blueimp file upload widget for the Yii framework",
    "keywords": [
        "yii",
        "yii2",
        "yii 2",
        "widget",
        "upload"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/tpoxa/yii2-file-upload-widget"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.0",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "2amigos/yii2-gallery-widget": "~1.0",
        "bower-asset/blueimp-file-upload": "~9.9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "dosamigos\\fileupload\\": "src"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
